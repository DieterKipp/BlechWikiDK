<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/fundstellentextsearch_Lied"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Titel"
        android:textColor="@color/design_default_color_primary_dark"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fundstellentextsearch_divider2" />


    <TextView
        android:id="@+id/fundstellentextsearch_Rank"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Rank"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fundstellentextsearch__divider" />

    <TextView
        android:id="@+id/fundstellentextsearch_Ix"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Ix"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/fundstellentextsearch_Rank"
        app:layout_constraintTop_toTopOf="@+id/fundstellentextsearch_Rank" />

    <TextView
        android:id="@+id/fundstellentextsearch_IxUr"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="IxUr"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/fundstellentextsearch_Ix"
        app:layout_constraintTop_toTopOf="@+id/fundstellentextsearch_Ix" />

    <View
        android:id="@+id/fundstellentextsearch_divider2"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_marginTop="16dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:id="@+id/fundstellentextsearch__divider"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fundstellentextsearch_Lied" />


</androidx.constraintlayout.widget.ConstraintLayout>